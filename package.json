{
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "live-server": "^1.2.2",
    "postcss": "^8.4.17",
    "tailwindcss": "^3.1.8",
    "concurrently": "^7.4.0"
  },
  "scripts": {
    "dev:start": "tailwindcss -i ./main.css -o ./output.css --watch",
    "dev:live": "live-server",
    "build": "tailwindcss -o output.css --minify",
    "dev": "concurrently --kill-others npm:dev:*"
  }
}
